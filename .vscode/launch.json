{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C/C++: Build and Debug Project",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/tem_core/xtem45_biogeo",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build with Makefile.xtem"
        }
    ]
}
