{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/tem_core/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "cppStandard": "c++11",
            "intelliSenseMode": "gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            },
            "compilerArgs": [
                "-fdiagnostics-color=always"
            ]
        }
    ],
    "version": 4
}

